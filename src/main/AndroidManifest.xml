            android:exported="true"
            android:process=":sync_purchase">
            <intent-filter>
                <action android:name="android.content.SyncAdapter" />
            </intent-filter>
    
            <meta-data
                android:name="android.content.SyncAdapter"
                android:resource="@xml/purchase_sync_adapter" />
        </service>
    
        <!-- PosOrderList Sync Provider & Service  // -->
        <provider
            android:name="com.ehealthinformatics.odoorxcore.device.providers.PosOrderSyncProvider"
            android:authorities="${applicationId}.odoorxcore.core.provider.content.sync.pos_order"
            android:label="@string/sync_label_pos_order"
            android:multiprocess="true" />
    
        <service
            android:name="com.ehealthinformatics.odoorxcore.device.services.PosOrderSyncService"
            android:exported="true"
            android:process=":sync_point_of_sale">
            <intent-filter>
                <action android:name="android.content.SyncAdapter" />
            </intent-filter>
    
            <meta-data
                android:name="android.content.SyncAdapter"
                android:resource="@xml/pos_sync_adapter" />
        </service>
    
        <!-- PosOrderLine Sync Provider & Service  // -->
        <provider
            android:name="com.ehealthinformatics.odoorxcore.device.providers.PosOrderLineSyncProvider"
            android:authorities="${applicationId}.odoorxcore.core.provider.content.sync.pos_order_line"
