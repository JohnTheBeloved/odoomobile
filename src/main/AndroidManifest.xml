            android:exported="true"
            android:process=":sync_pos_order_line">
            <intent-filter>
                <action android:name="android.content.SyncAdapter" />
            </intent-filter>
    
            <meta-data
                android:name="android.content.SyncAdapter"
                android:resource="@xml/pos_order_line_sync_adapter" />
        </service>
    
        <!-- PosConfigLine Sync Provider & Service  // -->
        <provider
            android:name="com.ehealthinformatics.odoorxcore.device.providers.PosConfigSyncProvider"
            android:authorities="${applicationId}.odoorxcore.core.provider.content.sync.pos_config"
            android:label="@string/sync_label_pos_config"
            android:multiprocess="true" />
    
        <service
